{
  "name": "default",
  "field_mappings": {
    "fecha": ["fecha", "fec", "date", "dia", "fecha operacion", "fecha valor"],
    "detalle": ["concepto", "detalle", "descripcion", "causal", "operacion", "movimiento"],
    "referencia": ["referencia", "ref", "nro", "numero", "comprobante", "transaccion", "documento", "nro de referencia", "nro referencia"],
    "debitos": ["debito", "debitos", "debe", "egreso", "salida", "cargo"],
    "creditos": ["credito", "creditos", "haber", "ingreso", "entrada", "abono", "deposito"],
    "saldo": ["saldo", "balance", "total", "saldo actual", "saldo final", "saldo al cierre"]
  },
  "date_patterns": [
    "\\d{1,2}/\\d{1,2}/\\d{2,4}",
    "\\d{4}-\\d{2}-\\d{2}"
  ],
  "amount_patterns": [
    "-?\\$?\\d{1,3}(?:[.,]\\d{3})*[.,]\\d{2}",
    "-?\\$?\\d+[.,]\\d{2}"
  ],
  "exclude_keywords": [
    "producto","cuenta","cbu","moneda","apellido","cuit","titulares","cantidad de titulares",
    "pagina","hoja","resumen","detalle de las cuentas","detalle de titulares",
    "movimientos pendientes","saldo del periodo anterior","saldo al cierre",
    "estimado cliente","banco patagonia","situacion impositiva"
  ],
  "table_required_keywords": ["fecha","concepto","debito","credito","saldo"],
  "min_header_score": 3,
  "date_format_hint": "DD/MM/YYYY",
  "preferred_column_order": ["fecha","detalle","referencia","debitos","creditos","saldo"],
  "amount_logic": {
    "mode": "sign",
    "debit_indicators": ["-"],
    "credit_indicators": ["+"],
    "negative_is_debit": true
  },
  "validation_rules": {
    "min_date_length": 8,
    "min_amount_digits": 1,
    "required_fields": ["fecha","detalle"],
    "skip_empty_rows": true,
    "skip_header_rows": true,
    "skip_summary_rows": true
  }
}
